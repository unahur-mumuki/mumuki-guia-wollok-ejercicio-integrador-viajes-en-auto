test "Trabajando para Ludmila, Melina pacta un precio de 15" {
  melina.estaTrabajandoPara(ludmila)
  assert.equals(15, melina.precioPactadoPorKm())
}

test "Trabajando para Ana MarÃ­a, Melina pacta un precio de 27" {
  melina.estaTrabajandoPara(anaMaria)
  assert.equals(27, melina.precioPactadoPorKm())
}

test "Trabajando para Teresa, Gabriela le cobra 72 a Melina por 2km" {
  melina.estaTrabajandoPara(teresa)
  assert.equals(72, gabriela.precioViaje(melina, 2))
}