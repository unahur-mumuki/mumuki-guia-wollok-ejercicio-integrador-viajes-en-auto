test "Roxana le cobra 180 a Ludmila por 10km" {
  assert.equals(180, roxana.precioViaje(ludmila, 10))
}

test "Roxana le cobra 220 a Teresa por 10km" {
  assert.equals(220, roxana.precioViaje(teresa, 10))
}

test "Roxana le cobra 36 a Ludmila por 2km" {
  assert.equals(180, roxana.precioViaje(ludmila, 10))
}

